{
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": false,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "quotmark": "single",
    /* regexp: true, */
    "undef": true,
    "unused": true,
    /* strict: true, //Added from site */
    "sub": true,
    "boss": true,
    //dont' allow assignments to be evaluated as truthy/falsey */
    "eqnull": true,
    //Allow == null
    "browser": true,
    "indent": 4,
    "smarttabs": true,
    "devel": true,
    "white": false,
    "onecase": true,
    "trailing": true,
    "maxparams": 6,
    "maxdepth": 9,
    "maxerr": 20,
    "globals": {
        "ActiveXObject": false,
        "define": false,
        "global": false,
        "module": false
    }
}
